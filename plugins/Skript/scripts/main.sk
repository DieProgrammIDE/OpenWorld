command /bcmessage [<text="">] [<text="">]:
	permission: skript.admin
	aliases: /bcmessages
	description: Broadcast-Nachrichten
	trigger:
		#send "%{bcmessages::*}%"
		if arg-1 == "":
			send formatted "Gib <tooltip:Klicke um den Command einzugeben><suggest command:/bcmessage add >&l/bcmessage add <Nachricht><reset> zum hinzufügen ein"
			send formatted "Gib <tooltip:Klicke um den Command einzugeben><suggest command:/bcmessage list>&l/bcmessage list<reset> zum Auflisten der Nachrichten"
			send formatted "Gib <tooltip:Klicke um den Command einzugeben><suggest command:/bcmessage remove >&l/bcmessage remove <Nummer><reset> zum löschen ein"
			#send formatted "Gib /bcmessage interval <Minuten> ein um die Wartedauer festzulegen"
		if arg-1 == "add":
			if arg-2 != "":
				add arg-2 to {bcmessages::*}
		if arg-1 == "list":
			loop {bcmessages::*}:
				send formatted "&b%loop-index%.&r %loop-value%"
		if arg-1 == "remove":
			if arg-2 != "":
				delete {bcmessages::%arg-2%}
		#if arg-1 == "interval":
		#	if arg-2 != "":
		#		set {bcmessage.interval} to arg-2
		#if arg-1 == "flush":
		#	delete {bcmessages::*}
		set {bcmessage.length} to 0
		loop {bcmessages::*}:
			add 1 to {bcmessage.length}
		send formatted "&aInsgesamt sind %{bcmessage.length}% Nachrichten eingespeichert"


every 5 minutes:
	set {_bcmessage.random} to random integer between 1 and {bcmessage.length}
	send formatted "%{bcmessages::%{_bcmessage.random}%}%" to all players


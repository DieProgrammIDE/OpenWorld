command /chunktooltip:
	description: Chunk-Boarders anzeigen
	trigger:
		set {_x.chunk} to rounded down player's x coordinate / 16
		set {_y.chunk} to rounded down player's y coordinate / 16
		set {_y.coord} to rounded player's y coordinate - 10
		set {_z.chunk} to rounded down player's z coordinate / 16
		set {_counter} to 1
		set {_counter2} to 1
		set {_x.chunk} to {_x.chunk} - 5
		set {_z.chunk} to {_z.chunk} - 5

		while {_counter} <= 10:
			send "test %{_counter}%"
			set {_counter} to {_counter} + 1
			while {_counter2} <= 10:
				send "test2 %{_counter2}%"
				set {_counter2} to {_counter2} + 1
				set {_x.chunkcorner} to {_x.chunk} * 16
				set {_x.chunkcorner2} to {_x.chunkcorner} + 16
				set {_y.chunkcorner} to {_y.chunk} * 16
				set {_z.chunkcorner} to {_z.chunk} * 16
				set {_z.chunkcorner2} to {_z.chunkcorner} + 16
				send "%{_x.chunk}% %{_y.chunk}% %{_z.chunk}%"
				send "%{_x.chunkcorner}% %{_y.chunkcorner}% %{_z.chunkcorner}%"
				play barrier at location at ({_x.chunkcorner}, {_y.coord}, {_z.chunkcorner}) to the player
				play barrier at location at ({_x.chunkcorner2}, {_y.coord}, {_z.chunkcorner}) to the player
				play barrier at location at ({_x.chunkcorner}, {_y.coord}, {_z.chunkcorner2}) to the player
				play barrier at location at ({_x.chunkcorner2}, {_y.coord}, {_z.chunkcorner2}) to the player
				set {_z.chunk} to {_z.chunk} + 1
			set {_x.chunk} to {_x.chunk} + 1
			set {_z.chunk} to rounded down player's z coordinate / 16
			set {_z.chunk} to {_z.chunk} - 5
			set {_counter2} to 0

		set {_counter} to 0

